hidden = [".config", "package-lock.json"]
compile = "clear"
run = "npm start"

[gitHubImport]
	requiredFiles = [".replit", "replit.nix", ".config", "package.json", "pnpm-lock.yaml"]



[nix]
	channel = "stable-22_11"


[packager]
	language = "nodejs-pnpm"

	[packager.features]
		packageSearch = true
		guessImports = true
		enabledForHosting = true

[deployment]
run = ["sh", "-c", "npm start"]
deploymentTarget = "cloudrun"